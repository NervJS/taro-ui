{
  "private": true,
  "name": "taro-ui",
  "description": "UI KIT for Taro",
  "author": "O2Team <aotu.io>",
  "homepage": "https://taro-ui.aotu.io",
  "repository": {
    "type": "git",
    "url": "git@github.com:nervjs/taro-ui.git"
  },
  "license": "MIT",
  "scripts": {
    "bootstrap": "pnpm i",
    "build": "pnpm -r --filter=./packages/* run build",
    "build:component": "pnpm run --scope taro-ui build",
    "clean": "rm -rf node_modules packages/*/node_modules",
    "clean:component": "pnpm remove --scope taro-ui clean",
    "publish": "lerna publish from-git --yes",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@commitlint/cli": "^8.3.6",
    "@commitlint/config-conventional": "^8.3.6",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "9.8.8",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-config-o2team": "^0.1.7",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-taro": "~3.6.15",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.21.0",
    "tslib": "2.5.0",
    "typescript": "~4.1.6"
  },
  "resolutions": {
    "sharp": "^0.28.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --quiet --cache --fix",
    "src/**/*.scss": "stylelint --syntax scss --fix"
  }
}
